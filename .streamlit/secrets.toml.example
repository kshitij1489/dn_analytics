# Streamlit Secrets Configuration
# Copy this file to .streamlit/secrets.toml and fill in your database credentials

[database]
url = "postgresql://username:password@localhost:5432/analytics"

# Alternative: Use individual parameters
# [database]
# host = "localhost"
# port = 5432
# database = "analytics"
# user = "your_username"
# password = "your_password"

